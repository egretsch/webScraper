<nav>
    <div class="nav-wrapper navstyle">
        <a href="#!" class="brand-logo">The web screper</a>
        <ul class="right hide-on-med-and-down">
            <li><a class="waves-effect waves-light btn button">Load new Article</a></li>
            <li><a href="/savedarticle" class="waves-effect waves-light btn button">Saved Article</a></li>

        </ul>
    </div>
</nav>

{{#each articles}} {{#unless saved}}
{{#if title}}
<figure class="storyPanel clearfix">
    <a href="{{link}}" class="pull-right" target="_blank">
        <div class="imgBox">
            <img src="{{img}}" alt="" class="storyImg" height="150">
        </div>
    </a>
    <a href="{{link}}" class="storyLink" target="_blank">
        <h2 class="storyHeadline">{{title}}</h2>
    </a>
    <h6 class="storySummary">{{summary}}</h6>
    <button class="storyButton" id="saveArticle" data-id="{{_id}}">Save Article</button>
</figure>
{{/if}}
{{/unless}} {{/each}}