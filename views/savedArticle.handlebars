<nav>
    <div class="nav-wrapper navstyle">
        <a href="#!" class="brand-logo">The web screper</a>
        <ul class="right hide-on-med-and-down">
            <li><a id="home" class="waves-effect waves-light btn button">New Articles</a></li>

        </ul>
    </div>
</nav>

{{#each articles}} {{#if saved}}
<figure class="storyPanel clearfix">
    <a href="{{link}}" class="pull-right" target="_blank">
        <div class="imgBox">
            <img src="{{img}}" alt="" class="storyImg" height="150">
        </div>
    </a>
    <a href="{{link}}" class="storyLink" target="_blank">
        <h2 class="storyHeadline">{{title}}</h2>
    </a>
    <h6 class="storySummary">{{summary}}</h6>
    <button class="storyButton" id="" data-id="{{_id}}">Add Comment</button>
    <button class="storyButton deleteArticle" id="" data-id="{{_id}}">Delete</button>
</figure>
{{/if}} {{/each}}